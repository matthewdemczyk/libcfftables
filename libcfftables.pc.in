prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=@CMAKE_INSTALL_FULL_LIBDIR@
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@

Name: libcfftables
Description: Cover-Free Family (CFF) construction and manipulation library
URL: https://github.com/matthewdemczyk/libcfftables
Version: @PROJECT_VERSION@
Requires.private: flint
Libs: -L${libdir} -lcfftables
Libs.private: -lm
Cflags: -I${includedir}